<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cst3104.androidlabs">  <!-- Update package name to match your project -->

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar">

        <!-- SecondActivity declaration -->
        <activity
            android:name=".SecondActivity"
            android:label="SecondActivity"
            android:exported="false" />

        <!-- MainActivity declaration, set as launcher activity -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

    <!-- Add this permission to allow phone calls -->
    <uses-permission android:name="android.permission.CALL_PHONE" />

</manifest>  <!-- Closing the manifest correctly here -->
